{
  "name": "hlPhoneBar",
  "version": "1.0.8",
  "description": "hl95 Phone Bar SDK",
  "keywords": [
    "cit",
    "sipphone",
    "websocket",
    "PhoneBar"
  ],
  "scripts": {
    "dev": "webpack-dev-server --open --config build/webpack.dev.conf.js --mode development",
    "start": "npm run dev",
    "build": "webpack --config ./build/webpack.base.conf.js --progress --colors",
    "build:dev": "webpack --progress --colors --config ./build/webpack.dev.conf.js --mode development ",
    "build:prod": "webpack --progress --colors --config ./build/webpack.prod.conf.js --mode production",
    "analyz": "cross-env npm_config_report=true npm run build:prod",
    "jsdoc": "jsdoc -c .jsdoc.json --package ./package.json -d ./doc/jsdoc -r"
  },
  "author": "zhangsx@9500.cn",
  "dependencies": {
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
